{
  "name": "structlint",
  "version": "0.1.0",
  "description": "CLI tool that analyzes and cleans up messy project file structures",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "structlint": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "CLAUDE.md"
  ],
  "keywords": [
    "cli",
    "refactoring",
    "file-structure",
    "ast",
    "code-analysis",
    "linter",
    "code-quality",
    "project-structure",
    "circular-dependencies",
    "dead-code"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.3.8",
    "madge": "^7.0.0",
    "ora": "^8.0.1",
    "simple-git": "^3.24.0",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.5",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.12.0",
    "typescript": "^5.4.0"
  }
}
